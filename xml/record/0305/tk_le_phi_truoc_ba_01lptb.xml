            <tax_prefix>B</tax_prefix>
            <xml_tag_in_db>ben_nhan_chuyen_nhuong</xml_tag_in_db>
        </replace>
        <replace>
            <from_xml_data>true</from_xml_data>
            <data_format>text</data_format>
            <find_string>#DIA_CHI_DAT#</find_string>
            <xml_tag_in_db>dia_chi_thua_dat</xml_tag_in_db>
        </replace>
        <replace>
            <from_xml_data>true</from_xml_data>
            <data_format>tabletostring</data_format>
