            <xml_tag_in_db>nguon_goc_su_dung</xml_tag_in_db>
        </replace>
        <replace>
            <from_xml_data>true</from_xml_data>
            <data_format>if_else</data_format>
            <field_list>registor_name,registor_name_other</field_list>
            <find_string>#NAME_A#</find_string>
        </replace>
        <replace>
            <from_xml_data>true</from_xml_data>
            <data_format>if_else</data_format>
            <field_list>registor_address,registor_address_other</field_list>
            <find_string>#DC_A#</find_string>
        </replace>
