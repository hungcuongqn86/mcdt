<?xml version="1.0" encoding="UTF-8"?>
<root>
	<replace_list>	
		<!--Nguoi nop ho so!-->	
		<replace>
			<from_xml_data>true</from_xml_data>
			<data_format>text</data_format>
			<find_string>#USER_NAME#</find_string>
			<xml_tag_in_db>nguoi_nop_ho_so</xml_tag_in_db>
			<field_name></field_name>
		</replace>
		<!--Don vi Nguoi nop ho so!-->	
		<replace>
			<from_xml_data>true</from_xml_data>
			<data_format>text</data_format>
			<find_string>#UNIT_ADDRESS#</find_string>
			<xml_tag_in_db>don_vi_nguoi_nop</xml_tag_in_db>
			<field_name></field_name>
		</replace>	
		<!--Thay the ten ho so!-->	
		<replace>
			<from_xml_data>false</from_xml_data>
			<data_format>text</data_format>
			<find_string>#RECORD_NAME#</find_string>
			<xml_tag_in_db></xml_tag_in_db>
			<field_name>C_NAME</field_name>
		</replace>
		<!--Thay the ten du an!-->	
		<replace>
			<from_xml_data>false</from_xml_data>
			<data_format>text</data_format>
			<find_string>#PROJECT_NAME#</find_string>
			<xml_tag_in_db></xml_tag_in_db>
			<field_name>C_TEN_DU_AN</field_name>
		</replace>		
		
		<!--Thay the ngay nhan ho so!-->
		<replace>
			<from_xml_data>false</from_xml_data>
			<data_format>isdate</data_format>
			<find_string>#RECEIVED_DATE#</find_string>
			<xml_tag_in_db></xml_tag_in_db>
			<field_name>C_RECEIVE_DATE</field_name>
		</replace>	
		<!--Thay the ten giai doan!-->
		<replace>
			<from_xml_data>false</from_xml_data>
			<data_format>text</data_format>
			<find_string>#PERIOE_NAME#</find_string>
			<xml_tag_in_db></xml_tag_in_db>
			<field_name>C_PERIOD_NAME</field_name>
		</replace>			
		<!--Thay the ngay hen tra ket qua!-->
		<replace>
			<from_xml_data>false</from_xml_data>
			<data_format>isdate</data_format>
			<find_string>#APPOINTED_DATE#</find_string>
			<xml_tag_in_db></xml_tag_in_db>
			<field_name>C_APPOINTED_DATE</field_name>
		</replace>
		<!--Tai lieu kem theo ho so!-->
		<replace>
			<from_xml_data>true</from_xml_data>
			<data_format>breakline</data_format>
			<find_string>#DOCUMENT_OF_RECORD#</find_string>
			<xml_tag_in_db>tai_lieu_kem_theo</xml_tag_in_db>
			<field_name></field_name>
		</replace>
	</replace_list>
</root>