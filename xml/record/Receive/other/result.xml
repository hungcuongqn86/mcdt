<?xml version="1.0" encoding="UTF-8"?>
<root>
	<common_para_list>
		<common_para>
			<list_form_title>HỒ SƠ CHỜ TRẢ KẾT QUẢ</list_form_title>
			<update_form_title>CẬP NHẬT THÔNG TIN TRẢ HỒ SƠ</update_form_title>
			<first_col_width>30%</first_col_width>
			<number_of_object_per_page>10</number_of_object_per_page>
			<js_file_name></js_file_name>
			<js_function>set_input()</js_function>
		</common_para>
	</common_para_list>
	<list_of_object>	
	</list_of_object>
	
	<!--Phan mo ta cau truc Form cap nhat doi tuong-->
	<update_object>
		<table_struct_of_update_form>
			<update_row_list>	<!--Phan mo ta cac dong Form cap nhat doi tuong-->
					<update_row>
						<have_line_before>true</have_line_before>
						<tag_list>ngay_tra</tag_list>
						<row_id></row_id>
					</update_row> 
					<update_row>
						<have_line_before>false</have_line_before>
						<tag_list>nguoi_nhan</tag_list>
						<row_id></row_id>
					</update_row> 
					<update_row>
						<have_line_before>false</have_line_before>
						<tag_list>le_phi</tag_list>
						<row_id></row_id>
					</update_row> 		
					<update_row>
						<have_line_before>false</have_line_before>
						<tag_list>ghi_chu</tag_list>
						<row_id></row_id>
					</update_row> 													
			</update_row_list>
		</table_struct_of_update_form>
		<update_formfield_list>								
			<ngay_tra>					
				<label>Ngày trả hồ sơ</label>
				<type>textbox</type>
				<data_format>isdate</data_format>
				<optional>false</optional>
				<width>20%</width>
				<width>20%</width>
				<row>3</row>
				<max_length>200</max_length>
				<message>NGAY TRA HO SO khong dung dinh dang ngay/thang/nam!</message>
				<note></note>
				<js_function_list></js_function_list>
				<js_action_list></js_action_list>
				<xml_data>true</xml_data>
				<column_name></column_name>
				<xml_tag_in_db>ngay_tra</xml_tag_in_db>
				<readonly_in_edit_mode>false</readonly_in_edit_mode>
				<disabled_in_edit_mode>false</disabled_in_edit_mode>
			</ngay_tra>
			
			<nguoi_nhan>					
				<label>Người nhận hồ sơ</label>
				<type>textbox</type>
				<data_format>text</data_format>
				<optional>false</optional>
				<width>20%</width>
				<width>100%</width>
				<row>3</row>
				<max_length>200</max_length>
				<message>NGUOI NHAN HO SO khong duoc de trong!</message>
				<note></note>
				<js_function_list></js_function_list>
				<js_action_list></js_action_list>
				<xml_data>true</xml_data>
				<column_name></column_name>
				<xml_tag_in_db>nguoi_nhan</xml_tag_in_db>
				<readonly_in_edit_mode>false</readonly_in_edit_mode>
				<disabled_in_edit_mode>false</disabled_in_edit_mode>
			</nguoi_nhan>	
			
			<le_phi>
				<label>Lệ phí</label>
				<type>textbox</type>
				<data_format>ismoney</data_format>
				<optional>false</optional>
				<width>20%</width>
				<max_length>100</max_length>
				<message>LE PHI khong dung dinh dang (phai la kieu so)!</message>
				<note> đồng</note>
				<only>true</only>
				<js_function_list></js_function_list>
				<js_action_list></js_action_list>
				<xml_data>true</xml_data>
				<column_name></column_name>
				<xml_tag_in_db>le_phi</xml_tag_in_db>
				<readonly_in_edit_mode>false</readonly_in_edit_mode>
				<disabled_in_edit_mode>false</disabled_in_edit_mode>
			</le_phi>	
							
			<ghi_chu>					
				<label>Ghi chú</label>
				<type>textarea</type>
				<data_format></data_format>
				<optional>true</optional>
				<only>false</only>
				<width>100%</width>
				<row>3</row>
				<max_length>200</max_length>
				<message>GHI CHU khong duoc de trong!</message>
				<note></note>
				<js_function_list></js_function_list>
				<js_action_list></js_action_list>
				<xml_data>true</xml_data>
				<column_name></column_name>
				<xml_tag_in_db>ghi_chu</xml_tag_in_db>
				<readonly_in_edit_mode>false</readonly_in_edit_mode>
				<disabled_in_edit_mode>false</disabled_in_edit_mode>
			</ghi_chu>				
		</update_formfield_list>
		<update_sql></update_sql>
	</update_object>
	<delete_object>
		<delete_sql></delete_sql>
	</delete_object>
</root>