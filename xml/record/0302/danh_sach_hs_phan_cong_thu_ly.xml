<?xml version="1.0" encoding="UTF-8"?>
<root>
	<common_para_list>
		<common_para>
			<form_title>TIẾP NHẬN HỒ SƠ MỚI</form_title>
			<first_col_width>25%</first_col_width>
			<js_file_name>js_functions_01.js</js_file_name>
			<js_function>set_input()</js_function>
		</common_para>
	</common_para_list>
	<list_of_object>
		<list_body> 
		<col>
			<label>&lt;input type="checkbox" name="chk_all_item_id" value=" onclick="checkbox_all_item_id(document.getElementsByName('chk_item_id'));"&gt;</label>
			<type>checkbox</type>
			<width>5%</width>
			<align>center</align>
			<xml_data>false</xml_data>
			<column_name>PK_RECORD</column_name>

		</col>
		<col>
			<label>Mã hồ sơ</label>
			<type>text</type>
			<width>15%</width>
			<align>left</align>
			<xml_data>false</xml_data>
			<column_name>C_CODE</column_name>

		</col>
			<col>
				<label>Bên chuyển</label>
				<type>json</type>
				<width>20%</width>
				<align>left</align>
				<xml_data>true</xml_data>
				<xml_tag_in_db>ben_chuyen_nhuong</xml_tag_in_db>
				<attr>ben_chuyen_nhuong_obj_name</attr>
			</col>
			<col>
				<label>Bên nhận</label>
				<type>json</type>
				<width>20%</width>
				<align>left</align>
				<xml_data>true</xml_data>
				<xml_tag_in_db>ben_nhan_chuyen_nhuong</xml_tag_in_db>
				<attr>ben_nhan_chuyen_nhuong_obj_name</attr>
			</col>
			<col>
				<label>Địa chỉ</label>
				<type>json</type>
				<width>20%</width>
				<align>left</align>
				<xml_data>true</xml_data>
				<xml_tag_in_db>ben_chuyen_nhuong</xml_tag_in_db>
				<attr>ben_chuyen_nhuong_obj_address</attr>
				<ItemIndex>0</ItemIndex>
			</col>
		<col>
			<label>Trạng thái</label>
			<type>text</type>
			<width>15%</width>
			<align>left</align>
			<xml_data>false</xml_data>
			<column_name>C_TEXT_STATUS</column_name>

		</col>
		<col>
			<label>Hạn xử lý</label>
			<type>text</type>
			<width>11%</width>
			<align>CENTER</align>
			<xml_data>false</xml_data>
			<column_name>OUT_OF_DATE</column_name>

		</col>
		<col>
			<label>Thông báo</label>
			<type>text</type>
			<width>15%</width>
			<align>left</align>
			<xml_data>false</xml_data>
			<column_name>C_MESSAGE</column_name>

		</col>
		</list_body>
	</list_of_object>
</root>