			<field_name></field_name>
			<xml_tag_in_db></xml_tag_in_db>
		</replace>
<!-- Một số thẻ khác-->
	     <replace>
			<from_xml_data>false</from_xml_data>
			<data_format>getdate</data_format>
			<find_string>#GET_DATE#</find_string>
